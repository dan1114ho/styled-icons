{
  "name": "styled-icons",
  "version": "0.1.0",
  "description": "Material Design Icons and Octicons available as Styled Components",
  "author": "Jacob Gillespie <jacobwgillespie@gmail.com>",
  "homepage": "https://github.com/jacobwgillespie/styled-icons",
  "repository": "git://github.com/jacobwgillespie/styled-icons.git",
  "license": "MIT",
  "main": "index.js",
  "typings": "./index.d.ts",
  "sideEffects": false,
  "files": [
    "material",
    "octicons",
    "CHANGELOG.md",
    "index.d.ts",
    "index.js",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "keywords": [
    "styled-components",
    "icons",
    "material-ui",
    "material",
    "octicons"
  ],
  "prettier": {
    "bracketSpacing": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "scripts": {
    "build": "./support/build.sh",
    "build-website": "./support/build-website.sh",
    "clean": "rm -rf build dist icons material octicons index.d.ts index.js",
    "dev-website": "parcel --out-dir build/website support/website/index.html",
    "serve-website": "serve build/website"
  },
  "peerDependencies": {
    "react": "*",
    "styled-components": ">=3 <4"
  },
  "devDependencies": {
    "@types/fs-extra": "^5.0.2",
    "@types/node": "^9.6.5",
    "@types/number-to-words": "^1.2.0",
    "@types/react": "^16.3.11",
    "@types/react-dom": "^16.0.5",
    "builtin-modules": "^2.0.0",
    "copy-to-clipboard": "^3.0.8",
    "fast-case": "^1.1.0",
    "fast-glob": "^2.2.0",
    "fs-extra": "^5.0.0",
    "material-design-icons": "^3.0.1",
    "number-to-words": "^1.2.3",
    "octicons": "^7.2.0",
    "parcel-bundler": "^1.7.1",
    "prettier": "^1.12.0",
    "react": "^16.3.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.3.2",
    "react-pose": "^1.5.2",
    "react-x-clipboard": "^2.5.0",
    "rollup": "^0.58.0",
    "rollup-plugin-auto-external": "^1.2.0",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-typescript2": "^0.13.0",
    "serve": "^6.5.5",
    "styled-components": "^3.2.5",
    "typescript": "^2.8.1"
  }
}
